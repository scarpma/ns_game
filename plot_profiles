rep 'data/ghia_v.dat' u 1:3 with points pt 30 lc "blue" t ""
rep 'data/ghia_u.dat' u 1:4 with points pt 30 lc "red" t "Re 1000"
rep 'data/ghia_v.dat' u 1:4 with points pt 30 lc "red" t ""

